{
    "name": "netemedia/component",
    "description": ":package_description",
    "license": "MIT",
    "authors": [
        {
            "name": "Lorenzo Milesi",
            "email": "lmilesi@netmedia.fr",
            "homepage": "https://netemedia.fr"
        }
    ],
    "homepage": "https://github.com/netemedia/component",
    "keywords": ["Laravel", "Component"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Netemedia\\Component\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netemedia\\Component\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Netemedia\\Component\\ComponentServiceProvider"
            ],
            "aliases": {
                "Component": "Netemedia\\Component\\Facades\\Component"
            }
        }
    }
}
